{
  "mcpServers": {
    "Homebrew": {
      "command": "brew",
      "args": ["mcp-server"]
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/var/tmp/change/this/path"
      ]
    }
  }
}
